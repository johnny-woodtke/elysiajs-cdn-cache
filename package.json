{
  "name": "elysiajs-cdn-cache",
  "description": "CDN caching plugin for Elysia - no more manually setting headers",
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "dev": "bun run --watch example/index.ts",
    "build": "bun build.ts",
    "release": "bun run build && npm publish"
  },
  "author": {
    "name": "johnny-woodtke",
    "url": "https://github.com/johnny-woodtke",
    "email": "johnny@woodtke.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnny-woodtke/elysiajs-cdn-cache.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "elysia",
    "cdn",
    "cache"
  ],
  "dependencies": {
    "elysia": "^1.3.1"
  },
  "devDependencies": {
    "@types/bun": "^1.2.13",
    "eslint": "9.6.0",
    "tsup": "^8.1.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "elysia": ">= 1.3.0"
  }
}
